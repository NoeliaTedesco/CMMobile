      <name>Txt18</name>
   </variable>
   <variable>
      <defaultValue>'de su página web o mediante la remisión de un correo electrónico a '</defaultValue>
      <description></description>
      <id>f3cc8f91-b036-449c-a05c-c457736001d7</id>
      <masked>false</masked>
      <name>Txt19</name>
   </variable>
